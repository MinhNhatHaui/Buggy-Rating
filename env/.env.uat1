# UAT1 Environment Configuration
TEST_ENV=uat1

# URLs
BASE_URL=https://uat1.buggy.justtestit.org

# Test Users
DEFAULT_USERNAME=uat1_tester
DEFAULT_PASSWORD=Uat1Test123!
ADMIN_USERNAME=uat1_admin
ADMIN_PASSWORD=Uat1Admin456!

# Test Execution
DEFAULT_TIMEOUT=90000
NAVIGATION_TIMEOUT=45000
ELEMENT_TIMEOUT=15000
NETWORK_TIMEOUT=8000

# Browser Settings
BROWSER=chromium
HEADLESS_MODE=false

# Test Configuration
ENABLE_SCREENSHOTS=true
ENABLE_VIDEO_RECORDING=true
SCREENSHOT_PATH=./test-results/uat1/screenshots
VIDEO_PATH=./test-results/uat1/videos

# API Settings
API_TIMEOUT=10000
API_RETRY_ATTEMPTS=3

# Reporting
ENABLE_HTML_REPORT=true
ENABLE_JSON_REPORT=true
REPORT_PATH=./test-results/uat1/reports